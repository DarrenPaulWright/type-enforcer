{
	"name": "type-enforcer",
	"version": "1.0.0-beta.3",
	"description": "A type enforcement library for javascript",
	"main": "index.js",
	"files": [
		"/src"
	],
	"sideEffects": false,
	"scripts": {
		"test": "karma start --single-run",
		"test-watch": "karma start",
		"bench": "karma start karma.bench.conf.js",
		"version": "npm run docs && git add docs",
		"docs:readme": "jsdoc2md index.js > README.md",
		"docs:enforce": "jsdoc2md src/enforcer/**/*.js > docs/enforce.md",
		"docs:method": "jsdoc2md src/methods/**/*.js > docs/method.md",
		"docs:checks": "jsdoc2md src/checks/**/*.js > docs/checks.md",
		"docs:queue": "jsdoc2md src/Queue.js > docs/Queue.md",
		"docs:enum": "jsdoc2md src/Enum.js > docs/Enum.md",
		"docs:removable": "jsdoc2md src/Removable.js > docs/Removable.md",
		"docs:castarray": "jsdoc2md src/utility/castArray.js > docs/castArray.md",
		"docs:applySettings": "jsdoc2md src/utility/applySettings.js > docs/applySettings.md",
		"docs:privateVars": "jsdoc2md src/utility/PrivateVars.js > docs/PrivateVars.md",
		"docs:equality": "jsdoc2md src/equality/*.js > docs/equality.md",
		"docs": "run-p \"docs:* -- -t docs/docs.hbs --plugin dmd-readable\""
	},
	"badges": [
		{
			"name": "npm",
			"image": "https://img.shields.io/npm/v/type-enforcer.svg",
			"url": "https://npmjs.com/package/type-enforcer"
		},
		{
			"name": "build",
			"image": "https://travis-ci.org/DarrenPaulWright/type-enforcer.svg?branch=master",
			"url": "https://travis-ci.org/DarrenPaulWright/type-enforcer"
		},
		{
			"name": "coverage",
			"image": "https://coveralls.io/repos/github/DarrenPaulWright/type-enforcer/badge.svg?branch=master",
			"url": "https://coveralls.io/github/DarrenPaulWright/type-enforcer?branch=master"
		},
		{
			"name": "deps",
			"image": "https://david-dm.org/darrenpaulwright/type-enforcer.svg",
			"url": "https://david-dm.org/darrenpaulwright/type-enforcer"
		},
		{
			"name": "size",
			"image": "https://packagephobia.now.sh/badge?p=type-enforcer",
			"url": "https://packagephobia.now.sh/result?p=type-enforcer"
		},
		{
			"name": "vulnerabilities",
			"image": "https://snyk.io/test/github/DarrenPaulWright/type-enforcer/badge.svg?targetFile=package.json",
			"url": "https://snyk.io/test/github/DarrenPaulWright/type-enforcer?targetFile=package.json"
		},
		{
			"name": "license",
			"image": "https://img.shields.io/github/license/DarrenPaulWright/type-enforcer.svg",
			"url": "https://npmjs.com/package/type-enforcer/LICENSE.md"
		}
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DarrenPaulWright/type-enforcer.git"
	},
	"keywords": [
		"type",
		"enforce",
		"method"
	],
	"author": "Darren Paul Wright <Darren@DarrenPaulWright.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/DarrenPaulWright/type-enforcer/issues"
	},
	"homepage": "https://github.com/DarrenPaulWright/type-enforcer#readme",
	"dependencies": {
		"object-agent": "^1.4.1"
	},
	"devDependencies": {
		"@babel/core": "^7.7.4",
		"@babel/preset-env": "^7.7.4",
		"dmd-readable": "^1.0.0",
		"jsdoc-to-markdown": "^5.0.3",
		"karma-webpack-bundle": "^0.0.4",
		"npm-run-all": "^4.1.5",
		"type-enforcer-test-helper": "^1.0.2",
		"webpack": "^4.41.2"
	}
}
