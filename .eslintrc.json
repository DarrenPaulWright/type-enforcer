{
	"parser": "babel-eslint",
	"plugins": [
		"babel"
	],
	"env": {
		"browser": true,
		"node": true,
		"es6": true,
		"mocha": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"extends": "eslint:recommended",
	"rules": {
		"curly": 2,
		"no-console": 0,
		"no-useless-escape": 0
	}
}
